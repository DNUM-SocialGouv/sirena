{
  "name": "@sirena/backend",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && cp -R generated dist/",
    "build:ci": "tsc -p tsconfig.build.json && cp -R generated dist/ && pnpm sentry:sourcemaps",
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "start": "node ./dist/index.js",
    "gql": "tsx src/gql.ts",
    "worker": "tsx src/start-worker.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:resolve": "prisma migrate resolve --rolled-back",
    "op:import:dematsocial": "tsx src/scripts/import_dematsocial.ts",
    "op:import:geodata": "tsx src/scripts/import_geodata.ts",
    "op:manage-api-keys": "tsx src/scripts/manage-api-keys.ts",
    "op:diff:autorite-competente:analyze": "tsx src/scripts/diff-autorite-competente.ts --csv \"prisma/documents/referentiel_autorite_competentes.csv\"",
    "op:diff:autorite-competente:up": "tsx src/scripts/diff-autorite-competente.ts --csv \"prisma/documents/referentiel_autorite_competentes.csv\" --dump-migration > migration.sql",
    "op:diff:autorite-competente:down": "tsx src/scripts/diff-autorite-competente.ts --csv \"prisma/documents/referentiel_autorite_competentes.csv\" --dump-migration --invert > down.sql",
    "op:diff:enums": "pnpm --filter @sirena/common --filter @sirena/backend-utils build && tsx src/scripts/diff_enums.ts",
    "db:diff": "prisma migrate diff --from-empty --to-schema-datamodel ./prisma/schema.prisma --script",
    "db:deploy": "prisma migrate deploy",
    "db:diff-down": "prisma migrate diff --from-schema-datamodel prisma/schema.prisma --to-schema-datasource prisma/schema.prisma --script > prisma/down.sql",
    "db:execute": "prisma db execute --schema prisma/schema.prisma --file",
    "db:reset": "prisma migrate reset --force",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "script:update-demat-social-mapper": "tsx scripts/updateDematSocial-mapper.ts",
    "generate:codegen": "graphql-codegen --config codegen.ts",
    "generate:openapi:thirdparty": "tsx src/swagger/genOpenAPISwagger.thirdparty.ts",
    "lint:staged": "git diff --cached --name-only --diff-filter=ACM | grep '^packages/backend/' | sed 's|^packages/backend/||' | grep -E '\\.(js|ts|tsx|jsx|json|md)$' | xargs -r pnpm biome check",
    "lint": "biome check",
    "sentry:sourcemaps": "sh -c 'SHORT=$(echo \"$APP_VERSION\" | cut -c1-7) && sentry-cli sourcemaps inject --release \"$SHORT\" --org incubateur --project psn-sirena-backend ./dist && sentry-cli --url https://sentry2.fabrique.social.gouv.fr/ sourcemaps upload --release \"$SHORT\" --org incubateur --project psn-sirena-backend ./dist'"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./hc": {
      "import": {
        "types": "./dist/hc.d.ts",
        "default": "./dist/hc.js"
      }
    }
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@hono/standard-validator": "^0.2.2",
    "@prisma/client": "^6.19.2",
    "@scalar/hono-api-reference": "^0.9.37",
    "@sentry/cli": "^2.58.4",
    "@sentry/node": "^9.47.1",
    "@sirena/backend-utils": "workspace:*",
    "@sirena/common": "workspace:*",
    "bullmq": "^5.67.1",
    "fhir": "^4.12.0",
    "file-type": "^21.3.0",
    "graffle": "8.0.0-next.173",
    "graphql": "^16.12.0",
    "hono": "catalog:",
    "hono-openapi": "catalog:",
    "hono-pino": "^0.10.3",
    "html-to-text": "^9.0.5",
    "ioredis": "5.9.2",
    "jsonwebtoken": "^9.0.3",
    "minio": "^8.0.6",
    "openid-client": "^6.8.1",
    "pdf-lib": "^1.17.1",
    "pino": "^9.14.0",
    "pino-pretty": "^13.1.3",
    "prom-client": "^15.1.3",
    "tmp-promise": "^3.0.3",
    "tsx": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/html-to-text": "^9.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "prisma": "^6.19.2",
    "typescript": "~5.9.3",
    "vitest": "catalog:",
    "vitest-mock-extended": "^3.1.0"
  }
}
