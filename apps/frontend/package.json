{
  "name": "@sirena/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "predev": "react-dsfr update-icons",
    "prebuild": "react-dsfr update-icons",
    "dev": "vite --host 0.0.0.0",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "build": "tsc -b && vite build",
    "lint": "biome check",
    "preview": "vite preview",
    "lint:staged": "git diff --cached --name-only --diff-filter=ACM | grep '^packages/frontend/' | sed 's|^packages/frontend/||' | grep -E '\\.(js|ts|tsx|jsx|json|md)$' | xargs -r pnpm biome check",
    "generate:openAPI": "tsx ./src/scripts/openAPI.ts"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.22.5",
    "@sirena/backend": "workspace:*",
    "@sirena/common": "workspace:*",
    "@sirena/ui": "workspace:*",
    "@tanstack/react-query": "^5.71.10",
    "@tanstack/react-router": "^1.115.0",
    "hono": "^4.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@playwright/test": "^1.52.0",
    "@tanstack/react-router-devtools": "^1.122.0",
    "@tanstack/router-plugin": "^1.115.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.19",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "globals": "^15.15.0",
    "jsdom": "^26.1.0",
    "openapi-typescript": "^7.6.1",
    "playwright": "^1.52.0",
    "typescript": "~5.7.2",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  }
}
