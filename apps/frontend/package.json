{
  "name": "@sirena/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "predev": "react-dsfr update-icons",
    "prebuild": "react-dsfr update-icons",
    "dev": "vite --host 0.0.0.0",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test": "vitest --typecheck",
    "build": "tsc -b && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "biome check",
    "preview": "vite preview",
    "lint:staged": "git diff --cached --name-only --diff-filter=ACM | grep '^packages/frontend/' | sed 's|^packages/frontend/||' | grep -E '\\.(js|ts|tsx|jsx|json|md)$' | xargs -r pnpm biome check",
    "generate:openAPI": "tsx ./src/scripts/openAPI.ts"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "catalog:",
    "@sentry/react": "^9.47.1",
    "@sirena/backend": "workspace:*",
    "@sirena/common": "workspace:*",
    "@sirena/ui": "workspace:*",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.159.4",
    "clsx": "^2.1.1",
    "hono": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "zod": "catalog:",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@playwright/test": "^1.58.0",
    "@sentry/vite-plugin": "^3.6.1",
    "@tanstack/react-router-devtools": "^1.157.15",
    "@tanstack/router-plugin": "^1.157.15",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "^4.7.0",
    "dotenv": "^17.2.3",
    "globals": "^16.5.0",
    "jsdom": "^26.1.0",
    "openapi-typescript": "^7.10.1",
    "playwright": "^1.58.0",
    "tsx": "catalog:",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "catalog:"
  }
}
