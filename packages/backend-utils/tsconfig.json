{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "declaration": true,
    "declarationMap": true,
    "allowImportingTsExtensions": false,
    "noEmit": false,
    "sourceMap": true,
    "baseUrl": "."
  },
  "include": ["src"]
}
